input(type="file" nv-file-select uploader="images.fileUploader" multiple)
button(ng-click="images.fileUploader.uploadAll();") {{'Upload All' | translate}}
ul
    li(ng-repeat="item in images.fileUploader.queue") Name: {{item.file.name}}
        div(ng-thumb='{ file: item._file, height: 200 }')
        button(ng-click="item.upload();") {{'Upload' | translate}}
        button(ng-click="item.remove();") {{'Remove' | translate}}